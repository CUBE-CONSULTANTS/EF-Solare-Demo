<mvc:View
	controllerName="com.sap.build.eeb1117ce-eu_2.untitledPrototype.controller.Consegna"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
<Page title="{i18n>}" showNavButton="true" navButtonPress="_onPageMenu">
		
    <VBox direction="Column" fitContainer="true" height="auto" justifyContent="Start" renderType="Bare" displayInline="false" class="sapUiSmallMarginTopBottom sapUiTinyMarginBeginEnd">
        <Label text="Consegna:" design="Bold" width="100%" />
        <Input value="" class="fullWidth" submit="cercaOrdine" valueStateText="Invalid entry"/>
        
    </VBox>
    <footer>
        <OverflowToolbar width="100%" height="46px" >
            <ToolbarSpacer/>
            <Button text="Avanti" width="auto" type="Default" press="cercaOrdine" iconDensityAware="false"/>
        </OverflowToolbar>
    </footer>
    <headerContent>
        <Image src="  ./resources/Logo.png" height="43px" width="80px" densityAware="true"/>
    </headerContent>
</Page>
</mvc:View>